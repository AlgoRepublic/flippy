<?xml version="1.0" encoding="utf-8"?>
<mx:Application xmlns:mx="http://www.adobe.com/2006/mxml" xmlns:comp="components.*" layout="vertical" verticalAlign="middle">
	<mx:Script>
		<![CDATA[
			public function authorized(event:Event):void {
				
				vs.selectedIndex = 1;
				
			}
		]]>
	</mx:Script>
	<mx:ViewStack id="vs" width="100%" height="100%">
		<comp:Login id="loginPage" authorized="authorized(event)"/>
		<comp:StartPage id="startPage" loginInfo="{loginPage.loginInfo}"/>
	</mx:ViewStack>
</mx:Application>
